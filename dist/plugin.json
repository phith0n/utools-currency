{
  "pluginName": "onlineExchangeRate",
  "description": "一个在线获取实时汇率信息的货币计算器",
  "version": "0.0.4",
  "author": "quericy",
  "contributors": ["ttionya <git@ttionya.com>"],
  "homepage": "https://quericy.me",
  "logo": "assets/logo.png",
  "main": "calculator.html",
  "pluginSetting": {
    "single": true
  },
  "features": [
    {
      "code": "base",
      "explain": "货币汇率计算器",
      "cmds": ["money", "exchange", "货币", "汇率", "计算器"]
    },
    {
      "code": "SGD",
      "explain": "货币汇率计算器",
      "cmds": [
        {
          "type": "regex",
          "label": "新加坡元汇率转换",
          "match": "/^(SGD|新加坡元).*/i"
        }
      ]
    },
    {
      "code": "CNY",
      "explain": "货币汇率计算器",
      "cmds": [
        {
          "type": "regex",
          "label": "人民币汇率转换",
          "match": "/^(CNY|RMB|中国元|人民币).*/i"
        }
      ]
    },
    {
      "code": "USD",
      "explain": "货币汇率计算器",
      "cmds": [
        {
          "type": "regex",
          "label": "美元汇率转换",
          "match": "/^(USD|美元).*/i"
        }
      ]
